{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@runtime/*": ["packages/runtime/*"],
      "@renderer/*": ["packages/renderer-semi/*"]
    }
  }
}
